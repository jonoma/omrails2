<div class="container">

	<p>
	  <strong>User:</strong>
	  <%= @article.user.email %>
	</p>

	<p>
	  <strong>List:</strong>
	  <%= @article.list.title %>
	</p>

	<p>
	  <strong>URL:</strong>
	  <%= @article.url %>
	</p>

	<% if @article.user == current_user %>
		<%= link_to 'Edit', edit_article_path(@article) %> |
		<%= link_to 'Back', articles_path %>
	<% end %>

</div>